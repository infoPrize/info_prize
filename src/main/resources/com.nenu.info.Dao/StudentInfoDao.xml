<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nenu.info.Dao.StudentInfoDao">
    
    <insert id="add" parameterType="com.nenu.info.common.entities.Student">
        INSERT INTO `t_student_info`
        VALUES (NULL, #{student.name}, #{student.sex}, #{student.stuNumber},
                #{student.grade}, #{student.majorCode}, #{student.phone}, NULL, NULL)
    </insert>

</mapper>